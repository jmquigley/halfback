{
  "name": "util.scaffold",
  "version": "0.0.31",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "A library to simplify SSH interactions with a remote server. It can be used to run commands, change config, create files, etc.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "debug": false,
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/util.scaffold"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/util.scaffold/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "dependencies": {
    "fs-extra": "^7.0.1",
    "ssh2": "0.7.0",
    "util.home": "0.0.26",
    "util.toolbox": "0.0.68",
    "util.wait": "0.0.33",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.4",
    "@types/node": "^10.12.18",
    "@types/ssh2": "^0.5.37",
    "@types/uuid": "^3.4.4",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "typescript-babel-jest": "^1.0.5",
    "util.fixture": "0.0.65",
    "util.pkgwrap": "^0.0.94"
  },
  "pkgwrap": {
    "cleanup": [
      "docs",
      "index.d.ts",
      "index.js"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "notify": false,
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "__tests__/helpers"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transform": {
      "^.+\\.ts?$": "typescript-babel-jest"
    },
    "verbose": false
  },
  "scripts": {
    "all": "yarn install && yarn run build && yarn test && yarn run docs",
    "build": "yarn clean && yarn run lint && pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage --jest",
    "docs": "pkgwrap --docs",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "test": "pkgwrap --testing --jest"
  }
}
