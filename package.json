{
  "name": "util.scaffold",
  "version": "0.0.29",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "A library to simplify SSH interactions with a remote server. It can be used to run commands, change config, create files, etc.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "debug": false,
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/util.scaffold"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/util.scaffold/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "dependencies": {
    "fs-extra": "^4.0.2",
    "ssh2": "0.5.5",
    "util.home": "0.0.22",
    "util.toolbox": "0.0.56",
    "util.wait": "0.0.30",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "4.0.5",
    "@types/node": "^8.0.54",
    "@types/ssh2": "^0.5.35",
    "@types/uuid": "^3.4.3",
    "ava": "^0.24.0",
    "coveralls": "^3.0.0",
    "nyc": "^11.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "util.fixture": "0.0.61",
    "util.pkgwrap": "^0.0.88"
  },
  "pkgwrap": {
    "cleanup": [
      "docs"
    ]
  },
  "scripts": {
    "all": "yarn install && yarn run build && yarn test && yarn run docs && yarn run report",
    "build": "yarn clean && yarn run lint && pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "docs": "pkgwrap --docs",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "report": "pkgwrap --reporting",
    "test": "pkgwrap --testing --ava"
  }
}
