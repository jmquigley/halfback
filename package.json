{
  "name": "util.scaffold",
  "version": "0.0.30",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "A library to simplify SSH interactions with a remote server. It can be used to run commands, change config, create files, etc.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "debug": false,
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/util.scaffold"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/util.scaffold/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "dependencies": {
    "fs-extra": "^5.0.0",
    "ssh2": "0.6.0",
    "util.home": "0.0.23",
    "util.toolbox": "0.0.58",
    "util.wait": "0.0.31",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.2",
    "@types/node": "^9.6.6",
    "@types/ssh2": "^0.5.35",
    "@types/uuid": "^3.4.3",
    "ava": "^0.25.0",
    "coveralls": "^3.0.0",
    "nyc": "^11.7.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "util.fixture": "0.0.62",
    "util.pkgwrap": "^0.0.91"
  },
  "pkgwrap": {
    "cleanup": [
      "docs"
    ]
  },
  "scripts": {
    "all": "yarn install && yarn run build && yarn test && yarn run docs && yarn run report",
    "build": "yarn clean && yarn run lint && pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "docs": "pkgwrap --docs",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "report": "pkgwrap --reporting",
    "test": "pkgwrap --testing --ava"
  }
}
