{
	"name": "util.scaffold",
	"version": "0.0.34",
	"author": "jmquigley <jmquigley@outlook.com>",
	"description": "A library to simplify SSH interactions with a remote server. It can be used to run commands, change config, create files, etc.",
	"debug": false,
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jmquigley/util.scaffold"
	},
	"bugs": {
		"url": "https://github.com/jmquigley/util.scaffold/issues"
	},
	"engines": {
		"node": ">=7.x"
	},
	"main": "./index.umd.min.js",
	"module": "./index.es.min.js",
	"types": "./index.d.ts",
	"files": [
		"index.*.min.js",
		"index.d.ts"
	],
	"dependencies": {
		"fs-extra": "^7.0.1",
		"ssh2": "0.8.2",
		"util.home": "0.0.30",
		"util.toolbox": "0.0.73",
		"util.toolbox-node": "^0.0.2",
		"util.wait": "0.0.36",
		"uuid": "^3.3.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.3.4",
		"@babel/plugin-transform-modules-umd": "^7.2.0",
		"@babel/preset-env": "^7.3.4",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/runtime": "^7.3.4",
		"@types/fs-extra": "5.0.5",
		"@types/jest": "^24.0.9",
		"@types/node": "^11.10.5",
		"@types/ssh2": "^0.5.38",
		"@types/uuid": "^3.4.4",
		"babel-minify": "^0.5.0",
		"babel-preset-minify": "^0.5.0",
		"coveralls": "^3.0.3",
		"jest": "^24.3.1",
		"prettier": "^1.16.4",
		"tslint": "^5.13.1",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.3.3333",
		"util.fixture": "0.0.69",
		"util.pkgwrap": "^0.0.104"
	},
	"pkgwrap": {
		"cleanup": [
			"docs"
		]
	},
	"scripts": {
		"all": "yarn install && yarn build && yarn docs && yarn test",
		"build": "yarn clean && yarn lint && yarn prettier && yarn build:es && yarn build:umd",
		"build:es": "tsc -p . && minify index.js --out-file index.es.min.js",
		"build:umd": "babel index.js --out-file index.umd.min.js",
		"clean": "pkgwrap --clean",
		"clean:dist": "yarn clean && rimraf node_modules/ yarn.lock",
		"coverage": "pkgwrap --coverage --jest",
		"docs": "pkgwrap --docs",
		"lint": "pkgwrap --lint",
		"prepublishOnly": "yarn run all",
		"prettier": "prettier --write \"**/*.{ts,tsx,js,json,yaml,yml,css}\"",
		"test": "jest"
	}
}
