{
  "name": "util.scaffold",
  "version": "0.0.22",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "A library to simplify SSH interactions with a remote server. It can be used to run commands, change config, create files, etc.",
  "debug": false,
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/util.scaffold"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/util.scaffold/issues"
  },
  "engines": {
    "node": ">=7.5"
  },
  "dependencies": {
    "fs-extra": "^3.0.1",
    "ssh2": "0.5.5",
    "util.home": "0.0.16",
    "util.pkgwrap": "0.0.57",
    "util.toolbox": "0.0.34",
    "util.wait": "0.0.25",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "3.0.2",
    "@types/node": "^7.0.23",
    "@types/ssh2": "^0.5.35",
    "@types/uuid": "^2.0.29",
    "ava": "^0.19.1",
    "coveralls": "^2.13.1",
    "nyc": "^11.0.1",
    "tslint": "^5.4.0",
    "typescript": "^2.3.4",
    "util.fixture": "0.0.54"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "all": "npm install && npm run clean && npm run lint && npm run build && npm run docs && npm test && npm run report",
    "build": "pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "docs": "pkgwrap --docs",
    "lint": "pkgwrap --lint",
    "postinstall": "pkgwrap --postinstall",
    "report": "pkgwrap --reporting",
    "test": "pkgwrap --testing --ava"
  },
  "pkgwrap": {
    "cleanup": [
      "docs"
    ]
  }
